    title 筑波大学ジャグリングサークル Sheep
    meta(charset="UTF-8")
    meta(name = "viewport", content = "width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no")
    meta(content = "", name = "description" )
    link(href = "style/bootstrap.min.css", media = "screen", rel = "stylesheet" )
    link(href = "style/style.css", media = "screen", rel = "stylesheet" )
    link(href = "components/fullcalendar/fullcalendar.css", media = "screen", rel = "stylesheet" )
    // script(src="//localhost:35729/livereload.js")
    script type="text/javascript"
      var _gaq = _gaq || [];
      _gaq.push(['_setAccount', 'UA-30508574-1']);
      _gaq.push(['_trackPageview']);

      (function() {
