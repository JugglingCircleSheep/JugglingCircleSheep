  body
    .navbar-wrapper
      .container
        .navbar.navbar-inverse.navbar-static-top
          .container
            .navbar-header
              button.navbar-toggle(type="button", data-toggle='collapse', data-target=".navbar-collapse")
                span.icon-bar
                span.icon-bar
                span.icon-bar
              a.navbar-brand Sheep
            .navbar-collapse.collapse
              ul.nav.navbar-nav
                li(ng-class="{active: $state.includes('home')}")
                  a(ui-sref='home') Home
                li(ng-class='{active: $state.includes("about")}')
                  a(ui-sref='about') About
                li(ng-class='{active: $state.includes("juggling")}')
                  a(ui-sref='juggling') Juggling
                li(ng-class='{active: $state.includes("member")}')
                  a(ui-sref='member.list') Member
                li(ng-class='{active: $state.includes("activity")}')
                  a(ui-sref='activity') Activity
                li(ng-class='{active: $state.includes("contact")}')
                  a(ui-sref='contact') Contact
              ul.nav.navbar-nav.navbar-right
                li
                  a(target='_blank',href='http://sheep.blog.jp/') Blog
                li
                  a(href='wiki/') Wiki
    .view
      .ui-view(ui-view)
    .container
      hr
      footer
        p.pull-right
          a(href='#').btn.btn-primary.btn-xs Back to top
        p
